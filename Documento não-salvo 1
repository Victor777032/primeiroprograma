int main() {
    int idade;
    printf("digite sua idade; ");
    scanf("%d" , &idade);
    if(idade >= 18){
        printf("\nIsso é uma festa, divirta-se!");   
    }else if (idade >=12){
        printf("\nVá beber uma fanta uva na barraquinha!");
    }else{
        printf("\nBeba chá de camomila!");
    }
   
   

    printf("\na sua idade e %d , idade");
    if(idade >= 65){
        printf("\nPasse na farmácia! ");
    }
   
   return 0;
}
